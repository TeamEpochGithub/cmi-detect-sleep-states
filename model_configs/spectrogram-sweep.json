{
    "name": "spectrogram-sweep",
    "data_info": {
        "window_size": 17280,
        "downsampling_factor": 1,
        "latitude": 40.730610,
        "longitude": -73.935242
    },
    "preprocessing": [
        {
            "kind": "mem_reduce",
            "id_encoding_path": "series_id_encoding.json"
        },
        {
            "kind": "similarity_nan",
            "as_feature": false
        },
        {
            "kind": "add_state_labels",
            "id_encoding_path": "series_id_encoding.json",
            "events_path": "data/raw/train_events.csv",
            "use_similarity_nan": true,
            "fill_limit": 8640,
            "nan_tolerance_window": 5
        },
        {
            "kind": "add_event_labels",
            "id_encoding_path": "series_id_encoding.json",
            "events_path": "data/raw/train_events.csv",
            "smoothing": 5,
            "steepness": 2
        },
        {
            "kind": "split_windows"
        }

    ],
    "feature_engineering": [
        {
            "kind": "rotation",
            "window_sizes": [18, 100, 360]   
        }
    ],

    "pretraining": {
        "test_size": 0.2,
        "scaler": {
            "kind": "standard-scaler",
            "copy": true
        }
    },

    "architecture": {
        "batch_size": 50,
        "bilinear": false,
        "early_stopping": 29,
        "encoder_name": "resnet34",
        "encoder_weights": "imagenet",
        "epochs": 66,
        "hidden_layers": 8,
        "hop_length": 18,
        "loss": "mse-torch",
        "lr": 0.0006118536185789334,
        "lr_schedule": {
          "cycle_limit": 2,
          "lr_min": 0.0014035626739418512,
          "t_initial": 15,
          "warmup_lr_init": 0.000001,
          "warmup_t": 7
        },
        "mask_unlabeled": false,
        "n_fft": 63,
        "optimizer": "adam-torch",
        "scale_factor": 2,
        "threshold": 0,
        "type": "Spectrogram_2D_Cnn",
        "use_auxiliary_awake": false
      }
}