{
    "name": "config",
    "log_to_wandb": false,
    "preprocessing": [
        "add_state_labels",
        "split_windows"
    ],
    "processed_loc_out": "data/processed",
    "processed_loc_in": "data/raw",
    "feature_engineering": {
        "kurtosis": {
            "window_sizes": [5, 10],
            "features": ["anglez", "enmo"]
        },
        "skewness": {
            "window_sizes": [5, 10],
            "features": ["anglez", "enmo"]
        },
        "mean": {
            "window_sizes": [5, 10],
            "features": ["anglez", "enmo"]
        }
    },
    "fe_loc_out": "data/features",
    "fe_loc_in": "data/processed",
    "pre_training": {
        "cv": 5,
        "standardize": "standard_scaler"
    },
    "models": {
        "ExampleModel": {
            "type": "example-fc-model",
            "epochs": 20,
            "batch_size": 32,
            "loss": "mae-torch",
            "optimizer": "adam-torch"
        }

    },
    "model_store_loc": "tm",
    "ensemble": {
        "models": ["ExampleModel"],
        "weights": [1],
        "comb_method": "addition"
    },
    "ensemble_loss": "mse-torch",
    "hpo": {
        "apply": true,
        "method": "example_hpo"
    },
    "cv": {
        "apply": true,
        "method": "example_cv"
    },
    "train_for_submission": false,
    "scoring": true
}