{
    "name": "config",
    "log_to_wandb": false,
    "pred_with_cpu": true,
    "train_series_path": "data/raw/train_series.parquet",
    "train_events_path": "data/raw/train_events.csv",
    "test_series_path": "data/raw/test_series.parquet",
    "preprocessing": [
        "mem_reduce",
        "add_state_labels",
        "split_windows",
        "add_segmentation_labels"
    ],
    "processed_loc_out": "data/processed",
    "processed_loc_in": "data/raw",
    "feature_engineering": {
        "rotation": {
            "window_sizes": [10,100,1000]
        }
    },
    "fe_loc_out": "data/features",
    "fe_loc_in": "data/processed",
    "pre_training": {
        "test_size": 0.2,
        "standardize": "standard"
    },
    "models": {
        "1D-Unet-CNN": {
            "type": "seg-unet-1d-cnn",
            "loss": "ce-torch",
            "optimizer": "adam-torch",
            "epochs": 15,
            "batch_size": 32,
            "lr": 0.001,
            "hidden_layers": 32,
            "early_stopping": 5
        }
    },
    "model_store_loc": "tm",
    "ensemble": {
        "models": ["1D-Unet-CNN"],
        "weights": [1],
        "comb_method": "addition"
    },
    "ensemble_loss": "mse-torch",
    "hpo": {
        "apply": false,
        "method": "example_hpo"
    },
    "cv": {
        "apply": false,
        "method": "example_cv"
    },
    "train_for_submission": true,
    "scoring": true
}
