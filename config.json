{
    "name": "config",
    "log_to_wandb": true,
    "pred_with_cpu": true,
    "train_series_path": "data/raw/train_series.parquet",
    "train_events_path": "data/raw/train_events.csv",
    "test_series_path": "data/raw/test_series.parquet",
    "processed_loc_out": "data/processed",
    "processed_loc_in": "data/raw",
    "model_store_loc": "tm",
    "fe_loc_out": "data/features",
    "fe_loc_in": "data/processed",
    "preprocessing": [
        {
            "kind": "mem_reduce",
            "id_encoding_path": "series_id_encoding.json"
        },
        {
            "kind": "split_windows",
            "start_hour": 15,
            "window_size": 17280
        },
        {
            "kind": "similarity_nan"
        },
         {
            "kind": "add_state_labels",
            "id_encoding_path": "series_id_encoding.json",
            "events_path": "data/raw/train_events.csv"
        }
    ],
    "feature_engineering": {
    },
    "pretraining": {
        "window_size": 17280,
        "test_size": 0.2,
        "downsample": {
            "factor": 12,
            "features": ["f_enmo", "f_anglez"],
            "methods": ["min", "max", "mean", "std", "median", "var"],
            "standard": "mean"
        },
        "scaler": {
            "kind": "minmax-scaler"
        }
    },
    "models": {
        "Classic-baseline": {
            "type": "classic-base-model",
            "median_window": 100,
            "threshold": 0.1,
            "use_nan_similarity": true
        }
    },
    "ensemble": {
        "models": ["Classic-baseline"],
        "weights": [1],
        "comb_method": "addition"
    },
    "ensemble_loss": "mse-torch",
    "hpo": {
        "apply": false,
        "method": "example_hpo"
    },
    "cv": {
        "apply": false,
        "method": "example_cv"
    },
    "scoring": true,
    "train_for_submission": true,
    "visualize_preds": {
        "n": 5,
        "browser_plot": false,
        "save": true
    }


}
